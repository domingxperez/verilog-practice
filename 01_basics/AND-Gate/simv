#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 9;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x57cfb3bcb240 .scope module, "and_gate_tb" "and_gate_tb" 2 3;
 .timescale -9 -9;
v0x57cfb3bdd3f0_0 .var "A", 0 0;
v0x57cfb3bdd4c0_0 .var "B", 0 0;
v0x57cfb3bdd590_0 .net "Y", 0 0, L_0x57cfb3bdd690;  1 drivers
S_0x57cfb3bcb3d0 .scope module, "uut" "and_gate" 2 8, 3 1 0, S_0x57cfb3bcb240;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "A";
    .port_info 1 /INPUT 1 "B";
    .port_info 2 /OUTPUT 1 "Y";
L_0x57cfb3bdd690 .functor AND 1, v0x57cfb3bdd3f0_0, v0x57cfb3bdd4c0_0, C4<1>, C4<1>;
v0x57cfb3bcb600_0 .net "A", 0 0, v0x57cfb3bdd3f0_0;  1 drivers
v0x57cfb3bdd210_0 .net "B", 0 0, v0x57cfb3bdd4c0_0;  1 drivers
v0x57cfb3bdd2d0_0 .net "Y", 0 0, L_0x57cfb3bdd690;  alias, 1 drivers
    .scope S_0x57cfb3bcb240;
T_0 ;
    %vpi_call 2 15 "$monitor", "Tiempo=%0t A=%b B=%b Y=%b", $time, v0x57cfb3bdd3f0_0, v0x57cfb3bdd4c0_0, v0x57cfb3bdd590_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57cfb3bdd3f0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57cfb3bdd4c0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57cfb3bdd3f0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57cfb3bdd4c0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57cfb3bdd3f0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57cfb3bdd4c0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57cfb3bdd3f0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57cfb3bdd4c0_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 22 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "and_gate_tb.v";
    "and_gate.v";
